<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>it.contrader.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>it.contrader.servlets.LogoutServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>NodesServlet</servlet-name>
		<servlet-class>it.contrader.servlets.NodesServlet</servlet-class>
	</servlet>	
	<servlet>
		<servlet-name>UserServlet</servlet-name>
		<servlet-class>it.contrader.servlets.UserServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>UtenteFinaleServlet</servlet-name>
		<servlet-class>it.contrader.servlets.UtenteFinaleServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>DossierServlet</servlet-name>
		<servlet-class>it.contrader.servlets.DossierServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>ProgettoServlet</servlet-name>
		<servlet-class>it.contrader.servlets.ProgettoServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>MomServlet</servlet-name>
		<servlet-class>it.contrader.servlets.MomServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/LoginServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/LogoutServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>NodesServlet</servlet-name>
		<url-pattern>/NodesServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UserServlet</servlet-name>
		<url-pattern>/UserServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>UtenteFinaleServlet</servlet-name>
		<url-pattern>/UtenteFinaleServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>DossierServlet</servlet-name>
		<url-pattern>/DossierServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProgettoServlet</servlet-name>
		<url-pattern>/ProgettoServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MomServlet</servlet-name>
		<url-pattern>/MomServlet</url-pattern>
	</servlet-mapping>
	
	
	
	<session-config>
		<session-timeout>4000</session-timeout>
	</session-config>
</web-app>