<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.5.xsd">
    <!--
        Added the constraints for entity City.
    -->
    <changeSet id="20190614101445-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="cities_id"
                                 baseTableName="city_image"
                                 constraintName="fk_city_image_cities_id"
                                 referencedColumnNames="id"
                                 referencedTableName="city"/>
        <addForeignKeyConstraint baseColumnNames="images_id"
                                 baseTableName="city_image"
                                 constraintName="fk_city_image_images_id"
                                 referencedColumnNames="id"
                                 referencedTableName="image"/>
        
    </changeSet>
</databaseChangeLog>
